<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/bg_home"
        android:clickable="true">


            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingBottom="60dp"
                    android:orientation="vertical">

                <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="233dp"
                        android:background="@drawable/bg_signup1">

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="233dp"
                            android:orientation="vertical"/>
                    <include layout="@layout/home_toolbar"/>

                    <LinearLayout
                            android:id="@+id/ln_info_home"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginBottom="20dp"
                            android:layout_gravity="bottom">

                        <de.hdodenhof.circleimageview.CircleImageView
                                xmlns:app="http://schemas.android.com/apk/res-auto"
                                android:id="@+id/img_profile_user"
                                android:layout_width="59.5dp"
                                android:layout_height="59.5dp"
                                android:src="@drawable/vlu_logo"
                                android:layout_marginLeft="22.1dp"/>

                        <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_marginLeft="16.3dp">

                            <TextView
                                    android:id="@+id/tv_name_user"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Trương Quốc Quân"
                                    android:textSize="23sp"
                                    android:textColor="#ffffff"
                            />

                            <TextView
                                    android:id="@+id/tv_email_user"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="T161552"
                                    android:textColor="#ffffff"
                                    android:textSize="14.4sp" />
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
                <LinearLayout
                        android:id="@+id/btnNext"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="15dp"
                        android:layout_marginTop="16.3dp"
                        android:background="#ffffff"
                        android:clickable="true">

                    <ImageView
                            android:id="@+id/imagehistory"
                            android:layout_width="42.3dp"
                            android:layout_height="42.3dp"
                            android:src="@drawable/icon_to_list"
                            android:layout_marginLeft="16dp"/>

                    <TextView
                            android:id="@+id/tv1"
                            android:layout_width="310dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="9.5dp"
                            android:layout_marginTop="10dp"
                            android:layout_weight="1"
                            android:text="@string/listbike"
                            android:textColor="#4a4a4a"
                            android:textSize="14.4sp" />
                    <ImageView
                            android:id="@+id/image1"
                            android:layout_width="22dp"
                            android:layout_height="22dp"
                            android:src="@drawable/ic_arrow_right"
                            android:layout_marginTop="10dp"/>
                </LinearLayout>

                <LinearLayout
                        android:id="@+id/ln_map"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="15dp"
                        android:layout_marginTop="16.3dp"
                        android:background="#ffffff"
                        android:clickable="true">

                    <ImageView
                            android:layout_width="42.3dp"
                            android:layout_height="42.3dp"
                            android:src="@drawable/ic_map"
                            android:layout_marginLeft="16dp"/>

                    <TextView
                            android:id="@+id/tv_map"
                            android:layout_width="310dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="9.5dp"
                            android:layout_marginTop="10dp"
                            android:layout_weight="1"
                            android:text="@string/map"
                            android:textColor="#4a4a4a"
                            android:textSize="14.4sp" />
                    <ImageView
                            android:layout_width="22dp"
                            android:layout_height="22dp"
                            android:src="@drawable/ic_arrow_right"
                            android:layout_marginTop="10dp"/>
                </LinearLayout>





